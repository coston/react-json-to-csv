{
  "name": "react-json-to-csv",
  "version": "1.0.4",
  "description": "A react button component to easily generate csv downloads of your json data.",
  "files": [
    "/lib"
  ],
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "author": "Coston Perkins <coston.perkins@ua.edu> (https://coston.io)",
  "homepage": "https://github.com/coston/react-json-to-csv",
  "repository": {
    "type": "git",
    "url": "https://github.com/coston/react-json-to-csv.git"
  },
  "bugs": {
    "url": "https://github.com/coston/react-json-to-csv/issues"
  },
  "license": "MIT",
  "scripts": {
    "dev": "yarn build --watch & cd demo && rm -rf node_modules && yarn && yarn dev",
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json"
  },
  "keywords": [
    "react",
    "react-component",
    "json",
    "csv",
    "button",
    "download"
  ],
  "dependencies": {
    "json-to-csv-export": "^1.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.3",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
